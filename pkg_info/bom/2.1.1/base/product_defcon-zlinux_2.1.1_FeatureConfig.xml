<?xml version="1.0"?>
<TIBCOInstallerFeatures>
	<productDef name="TIBCO Defcon 2.1.1" id="defcon-zlinux" version="2.1.1" releaseType="base" supportedPlatforms="linux/s390x" universalinstallerrelease="3.4.1" universalinstallerversion="V2" compatDisplayName="TIBCO Defcon" installDir="defcon/2.1" />

	<customVariables>
		<variable name="defcon.name" value="TIBCO Defcon"/>
		<variable name="defcon.home" value="${tibco.home}${fsp}defcon${fsp}2.1"/>
		<variable name="defcon.home.esc" value="${tibco.home.esc}/defcon/2.1"/>
		<variable name="defcon.version" value="2.1.1" />
		<variable name="defcon.short.version" value="2.1"/>
		<variable name="defcon.pi" value="init_value"/>
		<variable name="minimum.tra.version" value="5.7.1"/>
		<variable name="minimum.designer.version" value="5.7.1"/>
	</customVariables>

	<installerFeature name="Runtime" version="2.1.1" installLocation="${defcon.home}">
		<assemblyList>
			<assembly uid="product_tibco_defcon_runtime" version="2.1.1"></assembly>			
		</assemblyList>
	</installerFeature>

	<installerFeature name="Design" version="2.1.1" installLocation="${defcon.home}">
		<assemblyList>
			<assembly uid="product_tibco_defcon_design" version="2.1.1"></assembly>
		</assemblyList>
	</installerFeature>

	<installerFeature name="Documentation" version="2.1.1" installLocation="${defcon.home}">
		<assemblyList>
			<assembly uid="product_tibco_defcon_docs" version="2.1.1"></assembly>
		</assemblyList>
	</installerFeature>
</TIBCOInstallerFeatures>
